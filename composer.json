{
    "name": "muhamadhhassan/laramost",
    "description": "This package includes Mattermost drivers for Laravel different features like logging and notifications",
    "type": "library",
    "authors": [
        {
            "name": "muhamadhhassan",
            "email": "muhamadhhassan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaraMost\\": "src",
            "LaraMost\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.7"
    },
    "require": {
        "monolog/monolog": "2.8",
        "guzzlehttp/guzzle": "^7.4",
        "composer/ca-bundle": "^1.3"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
